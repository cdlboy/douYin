            android:name=".example.PeopleActivity"
            android:label="@string/title_activity_people" />
        <activity
            android:name=".example.ShebeiActivity"
            android:label="@string/title_activity_shebei" />
        <activity
            android:name=".example.ShezhiActivity"
            android:label="@string/title_activity_shezhi" />
        <activity
            android:name=".example.TongjiActivity"
            android:label="@string/title_activity_tongji" />
        <activity
            android:name=".example.QiDongActivity"
            android:label="逗音物联">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".example.TuoGuanActivity"
            android:label="@string/title_activity_tuo_guan"
