        <activity
            android:name=".example.WangJi1Activity"
            android:label="@string/title_activity_wang_ji1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".example.WangJi2Activity" />
        <activity android:name=".example.WangJi3Activity" />
        <activity android:name=".example.ZhuCeActivity" />
        <activity
            android:name=".example.AboutActivity"
